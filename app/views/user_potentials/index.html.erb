<h1>Listing user_potentials</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_potentials.each do |user_potential| %>
      <tr>
        <td><%= user_potential.email %></td>
        <td><%= link_to 'Show', user_potential %></td>
        <td><%= link_to 'Edit', edit_user_potential_path(user_potential) %></td>
        <td><%= link_to 'Destroy', user_potential, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User potential', new_user_potential_path %>
